<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:jee="http://www.springframework.org/schema/jee"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd
		http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.5.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
	">



	<jee:jndi-lookup id="dataSource" jndi-name="jdbc/datasourceRef" lookup-on-startup="false" expected-type="javax.sql.DataSource"/>
	<jee:jndi-lookup id="connectionFactory" jndi-name="jms/factoryRef"/>
	<jee:jndi-lookup id="queueRegistreerMelding" jndi-name="jms/at0300/outputRef"/>
	<jee:jndi-lookup id="eisCTGFactory" jndi-name="eis/factoryRef"/>
	
	<context:property-placeholder location="classpath:/queries-aig.properties"/>



	<bean id="behandelenAanleidingDAO" class="nl.belastingdienst.aig.dao.BehandelenAanleidingDAOCtgImpl" >
	</bean>

	<bean id="registreerMeldingJmsGateway" class="nl.belastingdienst.aig.jms.RegistreerMeldingGatewayJMSImpl">
		<property name="jmsTemplate">
			<bean class="org.springframework.jms.core.JmsTemplate">
				<property name="connectionFactory" ref="connectionFactory" />
				<property name="defaultDestination" ref="queueRegistreerMelding" />
			</bean>
		</property>
	</bean>

	<bean id="handmatigBeoordelenMeldingDAO" class="nl.belastingdienst.aig.dao.HandmatigBeoordelenMeldingDAOCtgImpl">
		<property name="ctgBeoordelenMeldingDaoImpl" ref="ctgBeoordelenMeldingDAO" />
	</bean>
	
	<bean id="ctgBeoordelenMeldingDAO" class="nl.belastingdienst.aig.dao.CtgBeoordelenMeldingDaoImpl">
		<property name="connectionFactory" ref="eisCTGFactory" />
	</bean>
	
	<bean id="waardeDAO" class="nl.belastingdienst.aig.dao.WaardeDAOJdbcImpl">
		<property name="dataSource" ref="dataSource" />
		<property name="querySelectWhereBsnJaarDatumingangDatumgeregistreerd" value="${waarde.querySelectWhereBsnJaarDatumingangDatumgeregistreerd}" />
		<property name="querySelectWhereBsnJaar" value="${waarde.querySelectWhereBsnJaar}" />
		<property name="querySelectWhereBsn" value="${waarde.querySelectWhereBsn}" />
		<property name="querySelectAll" value="${waarde.querySelectAll}" />
	</bean>

	<bean id="onderzoekDAO" class="nl.belastingdienst.aig.dao.OnderzoekDAOJdbcImpl">
		<property name="dataSource" ref="dataSource" />
		<property name="querySelectWhereBsnJaarDatumingang" value="${onderzoek.querySelectWhereBsnJaarDatumingang}" />
		<property name="querySelectWhereBsnJaar" value="${onderzoek.querySelectWhereBsnJaar}" />
		<property name="querySelectWhereBsn" value="${onderzoek.querySelectWhereBsn}" />
		<property name="querySelectAll" value="${onderzoek.querySelectAll}" />
	</bean>

	<bean id="meldingDAO" class="nl.belastingdienst.aig.dao.MeldingDAOJdbcImpl">
		<property name="dataSource" ref="dataSource" />
		<property name="querySelectWhereBsnJaarBerichtkenmerkAig" value="${melding.querySelectWhereBsnJaarBerichtkenmerkAig}" />
		<property name="querySelectWhereBsnJaarBerichtkenmerkDerden" value="${melding.querySelectWhereBsnJaarBerichtkenmerkDerden}" />
		<property name="querySelectWhereStatusHandmatigeAfhandeling" value="${melding.querySelectWhereStatusHandmatigeAfhandeling}" />
		<property name="querySelectAll" value="${melding.querySelectAll}" />
	</bean>

	<bean id="betrokkeneDAO" class="nl.belastingdienst.aig.dao.BetrokkeneDAOJdbcImpl">
		<property name="dataSource" ref="dataSource" />
		<property name="querySelectWhereBsnJaar" value="${betrokkene.querySelectWhereBsnJaar}" />
		<property name="querySelectWhereBsn" value="${betrokkene.querySelectWhereBsn}" />
		<property name="querySelectAll" value="${betrokkene.querySelectAll}" />
	</bean>

	<bean id="aanleidingDAO" class="nl.belastingdienst.aig.dao.AanleidingDAOJdbcImpl">
		<property name="dataSource" ref="dataSource" />
		<property name="querySelectWhereMelding" value="${aanleiding.querySelectWhereMelding}" />
		<property name="querySelectWhereOnderzoek" value="${aanleiding.querySelectWhereOnderzoek}" />
		<property name="querySelectAll" value="${aanleiding.querySelectAll}" />
		<property name="querySelectWhereBerichtKenmerkAigselect" value="${aanleiding.querySelectWhereBerichtKenmerkAigselect}" />
		<property name="querySelectAllWhereLegeEinddatum" value="${aanleiding.querySelectAllWhereLegeEinddatum" />
		<property name="querySelectWhereBetrokkene" value="${aanleiding.querySelectWhereBetrokkene" />
	</bean>


</beans>
